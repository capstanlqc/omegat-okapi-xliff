</trans-unit>
<trans-unit id="2:1">
<source xml:lang="en">(Please select one response.)</source>
<target xml:lang="ar"></target>
</trans-unit>
<trans-unit id="3:1">
<source xml:lang="en">We do not have a computer at home</source>
<target xml:lang="ar"></target>
</trans-unit>
<trans-unit id="4:1">
