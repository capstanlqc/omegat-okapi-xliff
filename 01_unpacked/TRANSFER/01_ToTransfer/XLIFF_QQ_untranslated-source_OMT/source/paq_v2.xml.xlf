<target xml:lang="ar"></target>
</trans-unit>
<trans-unit id="5:1">
<source xml:lang="en">Less than 1 hour a day</source>
<target xml:lang="ar"></target>
</trans-unit>
<trans-unit id="6:1">
<source xml:lang="en">Between 1 and 2 hours a day</source>
<target xml:lang="ar"></target>
</trans-unit>
<trans-unit id="7:1">
<source xml:lang="en">Between 2 and 3 hours a day</source>
<target xml:lang="ar"></target>
</trans-unit>
<trans-unit id="8:1">
<source xml:lang="en">3 hours a day or more</source>
<target xml:lang="ar"></target>
</trans-unit>
